{
  "imports": {
    "hono": "jsr:@hono/hono@^4.10.4",
    "hono/streaming": "jsr:@hono/hono@^4.10.4/streaming",
    "hono/cors": "jsr:@hono/hono@^4.10.4/cors",
    "openai": "npm:openai@^4.77.0",
    "pinecone": "npm:@pinecone-database/pinecone@^3.0.3",
    "dotenv": "jsr:@std/dotenv@^0.225.0"
  },
  "tasks": {
    "start": "deno run --unstable-kv --allow-net --allow-env --allow-read main.ts",
    "dev": "deno run --watch --unstable-kv --allow-net --allow-env --allow-read main.ts",
    "index": "deno run --unstable-kv --allow-net --allow-env --allow-read scripts/index-docs.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx",
    "lib": [
      "deno.window",
      "deno.unstable"
    ]
  }
}