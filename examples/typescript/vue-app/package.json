{
  "name": "vue-app-example",
  "version": "0.1.0",
  "description": "Example Vue.js app with X402 payment support",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:vite": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix --cache",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "keywords": [
    "x402",
    "vue",
    "example"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@openlibx402/client": "workspace:*",
    "@openlibx402/core": "workspace:*",
    "@openlibx402/express": "workspace:*",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.87.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "solana-wallets-vue": "^0.7.1"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.37.0",
    "eslint-plugin-oxlint": "~1.23.0",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.23.0",
    "postcss": "^8.4.32",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  }
}